#!/usr/bin/env bash

YAHOO_1000=$1

GRAPLE=$2
UNIPROX=$3

test -f $GRAPLE && rm $GRAPLE
touch $GRAPLE
test -f $UNIPROX && rm $UNIPROX
touch $UNIPROX

function cleanup {
	kill -- -$BASHPID
}
trap cleanup 0

1>&2 echo $YAHOO_1000
1>&2 echo $GRAPLE
1>&2 echo $UNIPROX

uniformity-experiment $GRAPLE "graple" "itemset $YAHOO_1000" 2 1000000 &
uniformity-experiment $UNIPROX "uniprox --walks=25" "itemset $YAHOO_1000" 2 1000000 &
wait
trap '' 0

